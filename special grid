function setup() {
  createCanvas(400, 400);
}

function draw() {
  background(220);
  
  let pixel=25;
  var offset = 5;
  for (let x = offset; x < (7 * pixel)+offset; x+=pixel){
    rect(x+offset,0,pixel,pixel);
    
    for (let y = offset; y < (7 * pixel)+offset; y+=pixel){
      rect(x+offset,y+offset,pixel,pixel);
    }
  }
  
 
}
